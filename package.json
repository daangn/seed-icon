{
  "name": "@seed-design/icon",
  "version": "0.5.1",
  "repository": "https://github.com/daangn/seed-icon.git",
  "author": "junghyeonsu <jung660317@naver.com>",
  "license": "MIT",
  "packageManager": "yarn@3.5.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "svg",
    "drawable",
    "pdf",
    "lib"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js",
      "require": "./lib/*.cjs"
    }
  },
  "peerDependencies": {
    "react": "*"
  },
  "scripts": {
    "prepack": "yarn build",
    "generate:icona": "node ./icona.js",
    "generate:react-index": "node scripts/generate-react-index.cjs",
    "build": "yarn generate:react-index && yarn build:tsc && yarn build:js",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:js": "node build.cjs",
    "release": "yarn changeset publish",
    "cs-version": "changeset version && yarn install --no-immutable"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@icona/generator": "^0.3.0",
    "@icona/types": "^0.3.0",
    "@icona/utils": "^0.3.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "esbuild": "^0.18.6",
    "glob": "^10.3.0",
    "nanobundle": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svgo": "^3.0.2",
    "typescript": "^5.1.3"
  }
}
