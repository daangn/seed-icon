{
  "name": "@seed-design/icon",
  "version": "0.6.2",
  "repository": "https://github.com/daangn/seed-icon.git",
  "author": "junghyeonsu <jung660317@naver.com>",
  "license": "MIT",
  "packageManager": "yarn@4.3.1",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "svg",
    "drawable",
    "pdf",
    "xml"
  ],
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "prepack": "yarn build",
    "generate:icona": "node --no-warnings=ExperimentalWarning --loader ts-node/esm icona.ts",
    "release": "yarn changeset publish",
    "cs-version": "changeset version && yarn install --no-immutable"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@icona/generator": "^0.7.1",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@types/node": "^20.14.9",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.1.3"
  }
}
